      "source": [
        "def keep_consonants(word):\n",
        "    vowels = \"aeiou\"\n",
        "    result = \"\"\n",
        "    for char in word:\n",
        "        if char not in vowels:\n",
        "            result += char\n",
        "    return result\n",
        "\n",
        "print(keep_consonants(\"babas\"))"
      ]
    }
